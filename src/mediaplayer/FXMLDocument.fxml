<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="200" prefWidth="320" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mediaplayer.FXMLDocumentController">
    <children>
      <ButtonBar layoutX="14.0" layoutY="146.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
              <Button onAction="#playButton" text="Play" />
            <Button mnemonicParsing="false" onAction="#pauseButton" text="Pause" />
          <Button mnemonicParsing="false" onAction="#stopButton" text="Stop" />
        </buttons>
      </ButtonBar>
      <Label fx:id="actionStatusLabel" layoutX="40.0" layoutY="52.0" text="Action Status Label" />
      <Button layoutX="104.0" layoutY="85.0" mnemonicParsing="false" onAction="#openSong" text="Open" />
    </children>
</AnchorPane>
